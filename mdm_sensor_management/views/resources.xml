<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <template id="assets_backend" name="mdm_sensor_management_assets"
              inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <link rel="stylesheet"
                  href="/mdm_sensor_management/static/src/css/mdm_sensor_management_views.css"/>
            <link rel="stylesheet"
                  href="/mdm_sensor_management/static/lib/iconset/mdm_iconset.css"/>
        </xpath>
    </template>

    <template id="minimal_layout_mdm_sensor_management" inherit_id="report.minimal_layout">
        <xpath expr="//head" position="inside">
            <link rel="stylesheet" type="text/css"
             href="/mdm_sensor_management/static/lib/iconset/css/mdm_iconset.css"/>
        </xpath>
    </template>
</odoo>
