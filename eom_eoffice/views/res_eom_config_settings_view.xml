<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="res_eom_config_settings_view_form" model="ir.ui.view">
        <field name="name">res.eom.config.settings.inherited.view.form</field>
        <field name="model">res.eom.config.settings</field>
        <field name="inherit_id" ref="eom_authdnie.res_eom_config_settings_view_form"/>
        <field name="arch" type="xml">
              <xpath expr="//field[@name='editable_notes']" position="after">
                    <field name="choose_company"/>
                    <field name="sequence_electronicfile_code_id"
                           string="Sequence for code"
                           options="{'no_quick_create':True,'no_create_edit':True}"
                           style="width:300px"
                           required="1"/>
                    <field name="deadline" style="width:300px" required="1"/>
                    <field name="notification_deadline" style="width:300px" required="1"/>
                    <field name="max_size_attachments" style="width:300px" required="1"/>
                    <field name="sign_certificate_path" style="width:300px"/>
                    <field name="sign_certificate_password_path" style="width:300px"/>
                    <field name="email_for_notice" style="width:300px"/>
              </xpath>
        </field>
    </record>

</odoo>